<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
    <object class="wxPanel" name="PrimaryProps">
        <object class="wxBoxSizer" variable="BoxSizer1" member="yes">
            <object class="sizeritem">
                <object class="wxFlexGridSizer" variable="FlexGridSizer1" member="no">
                    <cols>2</cols>
                    <object class="spacer">
                        <flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                        <border>5</border>
                        <option>1</option>
                        <size>5,5</size>
                    </object>
                    <object class="spacer">
                        <flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                        <border>5</border>
                        <option>1</option>
                        <size>5,5</size>
                    </object>
                    <object class="sizeritem">
                        <object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="no">
                            <label>Brigh</label>
                        </object>
                        <flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                        <border>5</border>
                        <option>1</option>
                    </object>
                    <object class="sizeritem">
                        <object class="wxSlider" name="ID_SLIDER1" variable="m_brightness_slider" member="yes">
                            <value>50</value>
                            <style>wxSL_LABELS</style>
                            <size>140,40</size>
                            <enabled>0</enabled>
                            <handler function="OnSliderScrollThumbTrack" entry="EVT_COMMAND_SCROLL_THUMBTRACK" />
                            <handler function="OnSliderScrollThumbRelease" entry="EVT_COMMAND_SCROLL_THUMBRELEASE" />
                            <handler function="OnBrightnessSliderScrollChanged" entry="EVT_COMMAND_SCROLL_CHANGED" />
                        </object>
                        <flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                        <border>1</border>
                        <option>1</option>
                    </object>
                    <object class="sizeritem">
                        <object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="no">
                            <label>Gam</label>
                        </object>
                        <flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                        <border>5</border>
                        <option>1</option>
                    </object>
                    <object class="sizeritem">
                        <object class="wxSlider" name="ID_SLIDER2" variable="m_gamma_slider" member="yes">
                            <style>wxSL_LABELS</style>
                            <size>-1,40</size>
                            <enabled>0</enabled>
                            <handler function="OnSliderScrollThumbTrack" entry="EVT_COMMAND_SCROLL_THUMBTRACK" />
                            <handler function="OnSliderScrollThumbRelease" entry="EVT_COMMAND_SCROLL_THUMBRELEASE" />
                            <handler function="OnGammaSliderScrollChanged" entry="EVT_COMMAND_SCROLL_CHANGED" />
                        </object>
                        <flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                        <border>1</border>
                        <option>1</option>
                    </object>
                    <object class="spacer">
                        <flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                        <border>5</border>
                        <option>1</option>
                    </object>
                    <object class="spacer">
                        <flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                        <border>5</border>
                        <option>1</option>
                        <size>10,30</size>
                    </object>
                    <object class="sizeritem">
                        <object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="no">
                            <label>Gain</label>
                        </object>
                        <flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                        <border>5</border>
                        <option>1</option>
                    </object>
                    <object class="sizeritem">
                        <object class="wxSlider" name="ID_SLIDER3" variable="m_gain_slider" member="yes">
                            <style>wxSL_LABELS</style>
                            <size>-1,40</size>
                            <enabled>0</enabled>
                            <handler function="OnSliderScrollThumbTrack" entry="EVT_COMMAND_SCROLL_THUMBTRACK" />
                            <handler function="OnSliderScrollThumbRelease" entry="EVT_COMMAND_SCROLL_THUMBRELEASE" />
                            <handler function="OnGainSliderScrollChanged" entry="EVT_COMMAND_SCROLL_CHANGED" />
                        </object>
                        <flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                        <border>1</border>
                        <option>1</option>
                    </object>
                    <object class="sizeritem">
                        <object class="wxStaticText" name="ID_STATICTEXT6" variable="StaticText6" member="no">
                            <label>Shut</label>
                        </object>
                        <flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                        <border>5</border>
                        <option>1</option>
                    </object>
                    <object class="sizeritem">
                        <object class="wxChoice" name="ID_CHOICE1" variable="m_shutter_choice" member="yes">
                            <content>
                                <item>1/25</item>
                                <item>1/33</item>
                                <item>1/50</item>
                                <item>1/100</item>
                                <item>1/250</item>
                                <item>1/500</item>
                                <item>1/1000</item>
                                <item>1/1500</item>
                                <item>1/2500</item>
                                <item>1/5000</item>
                                <item>1/10000</item>
                            </content>
                            <enabled>0</enabled>
                            <handler function="OnShutterChoiceSelect" entry="EVT_CHOICE" />
                        </object>
                        <flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                        <border>5</border>
                        <option>1</option>
                    </object>
                </object>
                <flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                <border>5</border>
                <option>4</option>
            </object>
            <object class="sizeritem">
                <object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
                    <cols>1</cols>
                    <object class="sizeritem">
                        <object class="wxCheckBox" name="ID_CHECKBOX2" variable="m_checkbox_fullauto" member="yes">
                            <label>Full auto</label>
                            <handler function="OnCheckboxFullautoClick" entry="EVT_CHECKBOX" />
                        </object>
                        <flag>wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                        <border>5</border>
                        <option>1</option>
                    </object>
                    <object class="sizeritem">
                        <object class="wxCheckBox" name="ID_CHECKBOX1" variable="m_checkbox_autoexp" member="yes">
                            <label>Auto exp.</label>
                            <handler function="OnCheckboxAutoExpClick" entry="EVT_CHECKBOX" />
                        </object>
                        <flag>wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                        <border>5</border>
                        <option>1</option>
                    </object>
                    <object class="sizeritem">
                        <object class="wxRadioBox" name="ID_RADIOBOX2" variable="m_radiobox_fps" member="yes">
                            <label>FPS</label>
                            <content>
                                <item>5</item>
                                <item>10</item>
                                <item>15</item>
                                <item>20</item>
                                <item>25</item>
                                <item>30</item>
                                <item>60</item>
                            </content>
                            <default>-1</default>
                            <style>wxRA_SPECIFY_COLS</style>
                            <handler function="OnRadioboxFpsSelect" entry="EVT_RADIOBOX" />
                        </object>
                        <flag>wxALL|wxEXPAND|wxALIGN_BOTTOM|wxALIGN_CENTER_HORIZONTAL</flag>
                        <border>4</border>
                        <option>1</option>
                    </object>
                </object>
                <flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                <border>5</border>
                <option>2</option>
            </object>
        </object>
    </object>
</wxsmith>
